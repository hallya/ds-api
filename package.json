{
  "name": "@hallya/ds-api",
  "version": "1.0.0",
  "description": "A Node.js library for interacting with Synology Download Station API",
  "main": "index.js",
  "type": "module",
  "bin": {
    "ds-torrents": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "list": "node bin/cli.js list",
    "list:json": "node bin/cli.js list --json",
    "remove": "node bin/cli.js remove",
    "purge": "node bin/cli.js purge",
    "info": "node bin/cli.js info"
  },
  "keywords": [
    "synology",
    "download-station",
    "api",
    "torrents",
    "nas",
    "api-client",
    "automation",
    "file-management"
  ],
  "files": [
    "lib/",
    "bin/",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "author": "Lucien Dulac",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hallya/synology-download-station-api.git"
  },
  "bugs": {
    "url": "https://github.com/hallya/synology-download-station-api/issues"
  },
  "homepage": "https://github.com/hallya/synology-download-station-api#readme",
  "dependencies": {
    "dotenv": "^17.2.3",
    "joi": "^18.0.1",
    "winston": "^3.18.3"
  }
}
